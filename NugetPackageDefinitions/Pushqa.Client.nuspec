            <dependency id="Newtonsoft.Json" version="4.5.11" />
            <dependency id="Microsoft.AspNet.SignalR.Client" version="1.0.1" />
            <dependency id="Rx-Main" version="2.1.30214.0" />
        </dependencies>
    </metadata>
    <files>
